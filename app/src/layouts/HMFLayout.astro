---
import BaseLayout from "./BaseLayout.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <header>
    <slot name="header" />
  </header>

  <main>
    <slot name="main" />
  </main>
  <footer>
    <slot name="footer" />
  </footer>
</BaseLayout>
